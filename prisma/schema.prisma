generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  email    String  @unique
  name     String
  password String
  address  Address
}

type Address {
  street       String
  number       Int
  city         String
  state        String
  neighborhood String
  zip          String
  country      String
}
